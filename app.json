{
  "expo": {
    "name": "Nicecream.fm",
    "slug": "nicecream-fm",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "nicecream",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fm.nicecream.app",
      "appleTeamId": "Y26P6FJZS5",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "spotify",
          "music",
          "youtube",
          "vnd.youtube.music"
        ]
      },
      "associatedDomains": [
        "applinks:nicecream.fm",
        "applinks:www.nicecream.fm"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ECECEC",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "fm.nicecream.app",
      "permissions": [
        "INTERNET",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "nicecream.fm",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.nicecream.fm",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", { "origin": "https://nicecream.fm" }],
      [
        "expo-splash-screen",
        {
          "ios": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#ECECEC",
            "resizeMode": "cover",
            "enableFullScreenImage_legacy": true,
            "dark": {
              "image": "./assets/images/splash-dark.png",
              "backgroundColor": "#1A1A1A"
            }
          },
          "android": {
            "image": "./assets/images/splash-icon.png",
            "imageWidth": 288,
            "backgroundColor": "#ECECEC",
            "dark": {
              "image": "./assets/images/splash-icon-dark.png",
              "backgroundColor": "#1A1A1A"
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6a144976-a293-4158-9693-9be5e86e8f51"
      }
    },
    "owner": "sfpavel"
  }
}
